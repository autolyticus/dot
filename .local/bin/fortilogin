#!/usr/bin/env bash

magic=$(curl -L google.com 2>/dev/null | grep magic | cut -d' ' -f10 | cut -d\" -f2)
username='esd15i016'
password='1111'

if [ -n "$1" ]; then
	username="$1"
	password="$2"
fi

if (curl -d "magic=$magic&username=$username&password=$password" 192.168.2.1:1000 2>/dev/null | grep Suc); then
	exit 0
else
	echo "Magic = $magic"
	exit 1
fi
